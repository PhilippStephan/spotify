{
  "compileOnSave": false,
  "compilerOptions": {
    "outDir": "./dist/out-tsc",
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "noImplicitOverride": true,
    "noPropertyAccessFromIndexSignature": true,
    "noImplicitReturns": true,
    "noFallthroughCasesInSwitch": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "sourceMap": true,
    "declaration": false,
    "experimentalDecorators": true,
    "moduleResolution": "node",
    "importHelpers": true,
    "target": "ES2022",
    "module": "ES2022",
    "useDefineForClassFields": false,
    "lib": ["ES2022", "dom"],
    "paths": {
      "auth/api-data-access": ["libs/auth/api-data-access/src/index.ts"],
      "auth/domain": ["libs/auth/domain/src/index.ts"],
      "auth/ui-callback": ["libs/auth/ui-callback/src/index.ts"],
      "dashboard/domain": ["libs/dashboard/domain/src/index.ts"],
      "dashboard/ui-dashboard": ["libs/dashboard/ui-dashboard/src/index.ts"],
      "library/domain": ["libs/library/domain/src/index.ts"],
      "library/ui-library": ["libs/library/ui-library/src/index.ts"],
      "profile/domain": ["libs/profile/domain/src/index.ts"],
      "profile/ui-logout": ["libs/profile/ui-logout/src/index.ts"],
      "profile/ui-profile": ["libs/profile/ui-profile/src/index.ts"],
      "profile/util-fetch-data": ["libs/profile/util-fetch-data/src/index.ts"],
      "search/domain": ["libs/search/domain/src/index.ts"],
      "search/ui-search": ["libs/search/ui-search/src/index.ts"],
      "shared/domain": ["libs/shared/domain/src/index.ts"],
      "shared/ui-album": ["libs/shared/ui-album/src/index.ts"],
      "shared/ui-artist-card": ["libs/shared/ui-artist-card/src/index.ts"],
      "shared/ui-play-button": ["libs/shared/ui-play-button/src/index.ts"],
      "shared/ui-profile-picture": [
        "libs/shared/ui-profile-picture/src/index.ts"
      ],
      "shared/ui-track-list": ["libs/shared/ui-track-list/src/index.ts"],
      "shared/util-mapper": ["libs/shared/util-mapper/src/index.ts"],
      "side-navigation/domain": ["libs/side-navigation/domain/src/index.ts"],
      "side-navigation/ui-side-nav": [
        "libs/side-navigation/ui-side-nav/src/index.ts"
      ],
      "statistics/domain": ["libs/statistics/domain/src/index.ts"],
      "statistics/ui-shell": ["libs/statistics/ui-shell/src/index.ts"],
      "statistics/ui-top-artists": [
        "libs/statistics/ui-top-artists/src/index.ts"
      ],
      "statistics/ui-top-tracks": [
        "libs/statistics/ui-top-tracks/src/index.ts"
      ],
      "welcome/domain": ["libs/welcome/domain/src/index.ts"],
      "welcome/ui-welcome": ["libs/welcome/ui-welcome/src/index.ts"]
    },
    "baseUrl": ".",
    "rootDir": "."
  },
  "angularCompilerOptions": {
    "enableI18nLegacyMessageIdFormat": false,
    "strictInjectionParameters": true,
    "strictInputAccessModifiers": true,
    "strictTemplates": true
  },
  "exclude": ["node_modules", "tmp"]
}
